{
  "Serilog": {
    "Using": [],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId", "WithThreadId" ],
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "..\\logs\\exampleAPIPrimaryLog.json",
          "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }

      }
    ]
  },
  "AllowedHosts": "*"
}
